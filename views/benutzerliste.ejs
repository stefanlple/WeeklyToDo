<!DOCTYPE html>
<html>
    <head>
        <title>Benutzerliste</title>
    </head>
    <body>
        <table  border="1">
            <% for(var i =0; i < benutzerverwaltung.length; i++) { %>
                <tr>
                    <td><%= benutzerverwaltung[i].id %></td>
                    <td><%= benutzerverwaltung[i].benutzername %></td>
                    <td><%= benutzerverwaltung[i].passwort %></td>
                    <td><%= benutzerverwaltung[i].monday %></td>
                    <td><%= benutzerverwaltung[i].tuesday %></td>
                    <td><%= benutzerverwaltung[i].wednesday %></td>
                    <td><%= benutzerverwaltung[i].thursday %></td>
                    <td><%= benutzerverwaltung[i].friday %></td>
                    <td><%= benutzerverwaltung[i].saturday %></td>
                    <td><%= benutzerverwaltung[i].friday %></td>
                </tr>        
            <%} %>
        </table>
    </body>
</html>